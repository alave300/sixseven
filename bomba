#include <iostream>
#include <vector>
using namespace std;
int su(vector<int> a){
    int s=0;
    for(int i=0; i<a.size(); i++){
        if(a[i]%2==0){
            s= s+a[i];
        }
    }
    cout<<s<<endl;
    return s;
}
int main(){
    
int iz = 0;
int sk;
vector<int>  e= {1,3,5,7,8,8,9,};
vector<int>  t= {2,4,6,5,7,21,20};
while (iz != 1){
cout<<"Ievadi veselu skaitli";
cin<<sk;
e.push_back(sk);
cout<<"Vai vēlies turpināt 1=jā, 2"
}
su(e);
su(t);

}

   cout<<"ievadi skaitli"<<endl;
   cin>>skaitlis;
 for (int i=1; i<=skaitlis; i++){
 cout<<s1<<" ";
 temp = s1 + s2;
s1 = s2;
s2 = temp;

    }
    cout<<endl;
    int summa = temp;
    cout<<summa;
    
 }
